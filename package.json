{
  "name": "emfts",
  "version": "1.0.0",
  "description": "TypeScript interfaces for Eclipse EMF Core",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
