    "@types/node": "^24.10.1",
    "@types/sax": "^1.2.7",
    "husky": "^9.0.0",
    "sax": "^1.4.3",
    "typescript": "^5.0.0",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "sax": "^1.4.3"
  }
